<%= form_for @trade do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :book1_id %><br />
    <%= f.text_field :book1_id %>
  </p>
  <p>
    <%= f.label :book2_id %><br />
    <%= f.text_field :book2_id %>
  </p>
  <p>
    <%= f.label :user1_id %><br />
    <%= f.text_field :user1_id %>
  </p>
  <p>
    <%= f.label :user2_id %><br />
    <%= f.text_field :user2_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
